{
  "name": "think-npm",
  "version": "0.0.1",
  "description": "Thinkific NPM Tool",
  "main": "think.js",
  "scripts": {
    "start": "think.js",
    "test": "NODE_ENV=test  ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --colors  test/*.spec.js test/**/*.spec.js",
    "watch-test": "NODE_ENV=test  ./node_modules/.bin/nodemon ./node_modules/.bin/_mocha --colors  test/*.spec.js test/**/*.spec.js",
    "watch-ctest": "NODE_ENV=test  ./node_modules/.bin/nodemon ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --colors  test/*.spec.js test/**/*.spec.js",
    "check-coverage": "./node_modules/.bin/istanbul check-coverage --statements 71.43 --functions 0 --branches 50 --lines 71.43  test/*.spec.js test/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thinkific/think-npm.git"
  },
  "author": "Jesus Rodriguez<jesus@thinkific.com>",
  "private": true,
  "bugs": {
    "url": "https://github.com/thinkific/think-npm/issues"
  },
  "homepage": "https://github.com/thinkific/think-npm#readme",
  "devDependencies": {
    "istanbul": "^0.4.5",
    "mocha": "^3.3.0",
    "nodemon": "^1.11.0",
    "rewire": "^2.5.2",
    "should": "^11.2.1",
    "sinon": "^2.2.0"
  },
  "dependencies": {
    "async": "^2.4.0",
    "chalk": "^1.1.3"
  }
}
